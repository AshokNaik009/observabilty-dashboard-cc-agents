{
  "name": "agent-team-performance-insights",
  "version": "1.0.0",
  "description": "Monitor AI agent team performance in coding projects and get optimization recommendations",
  "main": "dist/index.js",
  "bin": {
    "agent-insights": "./dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc && cd frontend && npm run build",
    "dev": "tsc && concurrently \"node dist/server.js\" \"cd frontend && npm run dev\"",
    "dev:server": "tsc && node dist/server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "cli": "tsc && node dist/index.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "claude-code",
    "ai-agents",
    "performance",
    "multi-agent",
    "analytics",
    "optimization"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "express": "^4.22.1",
    "fs-extra": "^11.2.0",
    "ora": "^5.4.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.1",
    "@types/yargs": "^17.0.33",
    "concurrently": "^9.1.0",
    "typescript": "^5.7.3"
  }
}
